<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sh_crops_processes_vehicles_tree_view" model="ir.ui.view">
        <field name="name">sh.process.vehicles.tree</field>
        <field name="model">sh.process.vehicles</field>
        <field name="arch" type="xml">
            <tree>
                <field name="sh_agri_vehicles_id" />
                <!-- <field name="total_vehicle" /> -->
                <field name="begin_date" />
                <field name="finish_date" />
                <field name="description" />
            </tree>
        </field>
    </record>

    <record id="sh_crops_processes_vehicles_action" model="ir.actions.act_window">
        <field name="name">Crops Vehicles</field>
        <field name="res_model">sh.process.vehicles</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Click here to create new record.
            </p>
        </field>
    </record>
</odoo>