<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="sh_list_process_equipment_tree_view" model="ir.ui.view">
        <field name="name">sh.process.equipment.tree</field>
        <field name="model">sh.process.equipment</field>
        <field name="arch" type="xml">
            <tree>
                <field name="equipment" />
                <field name="serial_no" />
                <field name="alloted_date" />
                <field name="owner" />
                <field name="technician" />
                <field name="equipment_category" />
            </tree>
        </field>
    </record>

    <record id="sh_crops_processes_equipment_action" model="ir.actions.act_window">
        <field name="name">Crops Processes equipment</field>
        <field name="res_model">sh.process.equipment</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Click here to create new record.
            </p>
        </field>
    </record>

    
</odoo>