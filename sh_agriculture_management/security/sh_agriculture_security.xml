<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
    <record model="ir.module.category" id="sh_category_agriculture">
        <field  name="name">Agriculture</field>
        <field name="sequence">25</field>
    </record>



    <record id="group_worker" model="res.groups">
        <field name="category_id" ref="sh_category_agriculture"/>
